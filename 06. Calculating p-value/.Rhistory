m1 <- c(10,12,30)
m2 <- c(10.5,13,28.5)
sd1 <- c(3,4,4.5)
sd2 <- c(2.5,5.3,3)
num1 <- c(300,210,420)
num2 <- c(230,340,400)
se <- sqrt(sd1*sd1/num1+sd2*sd2/num2)
t <- (m1-m2)/se
pt(t,df=pmin(num1,num2)-1)
pt(-abs(t),df=pmin(num1,num2)-1)
